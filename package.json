{
  "private": true,
  "scripts": {
    "dev:turbo": "next dev --turbo --show-all",
    "dev": "next dev",
    "dev:tailwind": "concurrently \"next dev --turbo --show-all\" \"npm run tailwind -- --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/globals.css -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.0",
    "clsx": "1.2.1",
    "concurrently": "7.5.0",
    "cookies-next": "2.1.1",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.6",
    "framer-motion": "7.6.12",
    "next": "latest",
    "next-redux-wrapper": "8.0.0",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-easy-crop": "4.6.2",
    "react-image-crop": "10.0.8",
    "react-markdown": "8.0.3",
    "react-redux": "8.0.5",
    "rehype-stringify": "9.0.3",
    "remark-gfm": "3.0.1",
    "remark-parse": "10.0.1",
    "remark-rehype": "10.1.0",
    "smooth-scroll-into-view-if-needed": "1.1.33",
    "styled-components": "6.0.0-beta.2",
    "styled-jsx": "5.1.0",
    "tiny-emitter": "2.1.0",
    "use-count-up": "3.0.1"
  },
  "devDependencies": {
    "@types/crypto-js": "4.1.1",
    "@types/node": "18.0.3",
    "@types/ramda": "0.28.20",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "eslint": "8.19.0",
    "eslint-config-next": "latest",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
}
